Cell[TextData[{
 StyleBox["Now, we are aiming to solve the Cartan structure equations for the \
Levi-Civita connection. This means, we are searching for a 1-form ",
  FontFamily->"Linux Libertine O"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[<|"boxes" -> FormBox[
        RowBox[{"\[Alpha]", "\[LongEqual]", 
          SubscriptBox["\[Omega]", "1"], 
          StyleBox["d", "TI"], 
          StyleBox["x", "TI"], "+", 
          SubscriptBox["\[Omega]", "2"], 
          StyleBox["d", "TI"], 
          StyleBox["y", "TI"]}], TraditionalForm], "errors" -> {}, "input" -> 
      "$\\alpha = \\omega_1 dx + \\omega_2 dy$", "state" -> "Boxes"|>,
     "TeXAssistantTemplate"], " "}], TraditionalForm]],
  FontFamily->"Linux Libertine O",ExpressionUUID->
  "e5040f34-7aeb-4a4c-8076-fd57dafc976f"],
 StyleBox[" such that \n",
  FontFamily->"Linux Libertine O"],
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["d", "TI"], 
         RowBox[{"(", 
           Cell[
            BoxData[
             FormBox[
              GridBox[{{
                 Cell[
                  TextData[{" ", 
                    Cell[
                    BoxData[
                    SubsuperscriptBox[
                    StyleBox["e", "TI"], 
                    StyleBox["PPF", FontSlant -> "Plain"], "1"]], 
                    "InlineFormula"], " "}]]}, {
